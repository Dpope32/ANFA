{
  "name": "stock-price-predictor",
  "version": "1.0.0",
  "description": "Hybrid quantitative-qualitative stock price prediction system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:connections": "ts-node scripts/test-connections.ts",
    "verify": "ts-node scripts/verify-implementation.ts",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t stock-price-predictor .",
    "docker:run": "docker run -p 3000:3000 --env-file .env stock-price-predictor",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "packageManager": "pnpm@10.15.1",
  "keywords": [
    "stock",
    "prediction",
    "trading",
    "machine-learning"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0",
    "@types/compression": "^1.7.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.0"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "axios": "^1.6.0",
    "ioredis": "^5.3.0",
    "redis": "^4.6.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4"
  }
}
