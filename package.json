{
  "name": "stock-price-predictor",
  "version": "1.0.0",
  "description": "Hybrid quantitative-qualitative stock price prediction system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "dev:full": "concurrently \"pnpm run dev\" \"cd frontend && pnpm run dev\"",
    "redis:start": "powershell -ExecutionPolicy Bypass -File scripts/start-redis.ps1",
    "dev:all": "concurrently \"pnpm run redis:start\" \"pnpm run dev:full\"",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:connections": "ts-node scripts/test-connections.ts",
    "test:server": "ts-node scripts/test-server.ts",
    "test:app": "pnpm test:coverage && pnpm test:api && pnpm test:integration && pnpm test:config && pnpm lint && pnpm test:connections && pnpm test:coverage-report && pnpm test:server",
    "test:api": "jest --testPathPattern=api --coverage",
    "test:integration": "jest --testPathPattern=integration --coverage",
    "test:config": "jest --testPathPattern=config --coverage",
    "test:coverage-report": "jest --coverage --coverageReporters=text-summary --coverageReporters=lcov",
    "test:watch:coverage": "jest --watch --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "verify": "ts-node scripts/verify-implementation.ts",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t stock-price-predictor .",
    "docker:run": "docker run -p 3000:3000 --env-file .env stock-price-predictor",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --rmi all",
    "stock:fetch": "ts-node scripts/fetch-stock-data.ts",
    "stock:load": "ts-node scripts/load-stock-to-pocketbase.ts",
    "stock:pipeline": "ts-node scripts/stock-data-pipeline.ts",
    "stock:tesla": "ts-node scripts/stock-data-pipeline.ts -s TSLA -d 365 -c",
    "stock:tesla-strategic": "ts-node scripts/tesla-data-strategic.ts",
    "stock:load-supabase": "ts-node scripts/load-stock-data-to-supabase.ts",
    "stock:help": "ts-node scripts/stock-data-pipeline.ts --help"
  },
  "packageManager": "pnpm@10.15.1",
  "keywords": [
    "stock",
    "prediction",
    "trading",
    "machine-learning"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.0",
    "effect": "^3.17.13",
    "express": "^4.18.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "morgan": "^1.10.0",
    "redis": "^4.6.0"
  }
}